<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>com.tautulli.Tautulli</string>
    <key>ProgramArguments</key>
    <array>
      <string>/Applications/Tautulli.app/Tautulli.py</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <true/>
    <key>StandardErrorPath</key>
    <string>/usr/local/var/log/tautulli.log</string>
    <key>StandardOutPath</key>
    <string>/usr/local/var/log/tautulli.log</string>
  </dict>
</plist>
