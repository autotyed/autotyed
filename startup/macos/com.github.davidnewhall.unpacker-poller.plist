<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>com.github.davidnewhall.unpacker-poller</string>
    <key>ProgramArguments</key>
    <array>
      <string>/usr/local/bin/unpacker-poller</string>
      <string>-D</string>
      <string>-c</string>
      <string>/usr/local/etc/unpacker-poller/up.conf</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <true/>
    <key>StandardErrorPath</key>
    <string>/usr/local/var/log/unpacker-poller.log</string>
    <key>StandardOutPath</key>
    <string>/usr/local/var/log/unpacker-poller.log</string>
  </dict>
</plist>
